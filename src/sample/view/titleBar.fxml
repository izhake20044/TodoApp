<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<HBox alignment="TOP_RIGHT" focusTraversable="true" pickOnBounds="false" prefHeight="32.0" prefWidth="94.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.TitleBarController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Circle fx:id="hideButton" fill="#fffb0d" radius="6.0" stroke="BLACK" strokeType="INSIDE">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <effect>
            <Reflection />
         </effect>
      </Circle>
      <Circle fx:id="miniButton" disable="true" fill="#18ff00" opacity="0.35" radius="6.0" stroke="BLACK" strokeType="INSIDE">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <effect>
            <Reflection />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Circle>
      <Circle fx:id="exitButton" fill="RED" radius="6.0" stroke="BLACK" strokeType="INSIDE">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <effect>
            <Reflection />
         </effect>
      </Circle>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</HBox>
